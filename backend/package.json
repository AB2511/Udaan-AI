{
  "name": "udaan-ai-backend",
  "version": "1.0.0",
  "description": "Backend API for Udaan AI career guidance platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:ai": "node tests/run-ai-integration-tests.js",
    "test:ai:unit": "node tests/run-ai-integration-tests.js --suite=unit",
    "test:ai:integration": "node tests/run-ai-integration-tests.js --suite=integration",
    "test:ai:coverage": "node tests/run-ai-integration-tests.js --coverage",
    "test:ai:watch": "nodemon --exec 'npm run test:ai' --watch tests --watch services --watch controllers",
    "db:init": "node scripts/setupDatabase.js --init",
    "db:drop": "node scripts/setupDatabase.js --drop",
    "db:stats": "node scripts/setupDatabase.js --stats",
    "db:validate": "node scripts/setupDatabase.js --validate",
    "db:reset": "node scripts/setupDatabase.js --drop --init",
    "db:setup": "node scripts/setupDatabase.js --init --validate --stats",
    "models:validate": "node scripts/validateModels.js",
    "seed:questions": "node scripts/seedQuestionBank.js",
    "seed:prod": "node scripts/seedQuestionBank.js --production",
    "migrate": "echo 'No migrations to run'",
    "backup": "node -e \"require('./services/BackupService.js').createManualBackup()\"",
    "monitoring:start": "node ../scripts/monitoring-daemon.js",
    "backup:start": "node ../scripts/backup-daemon.js"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "file-type": "^21.0.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "mammoth": "^1.10.0",
    "mongodb": "^5.9.2",
    "mongoose": "^7.8.7",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.5.0",
    "form-data": "^4.0.4",
    "jest": "^29.5.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "career-guidance",
    "ai",
    "education",
    "nodejs",
    "express"
  ],
  "author": "Udaan AI Team",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "testTimeout": 30000,
    "preset": "default"
  }
}
