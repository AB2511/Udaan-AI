{
  "name": "udaan-ai-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run src/tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "vitest run src/tests/accessibility/",
    "test:complete": "npm run test:run && npm run test:integration && npm run test:accessibility",
    "test:ai": "vitest run src/tests/components/AIComponents.test.jsx src/tests/e2e/AIWorkflows.e2e.test.jsx",
    "test:ai:components": "vitest run src/tests/components/AIComponents.test.jsx",
    "test:ai:e2e": "vitest run src/tests/e2e/AIWorkflows.e2e.test.jsx",
    "test:ai:watch": "vitest src/tests/components/AIComponents.test.jsx src/tests/e2e/AIWorkflows.e2e.test.jsx",
    "test:ai:coverage": "vitest run --coverage src/tests/components/AIComponents.test.jsx src/tests/e2e/AIWorkflows.e2e.test.jsx",
    "analyze": "node analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze"
  },
  "dependencies": {
    "axios": "^1.12.1",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest-axe": "^8.0.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vite": "^4.3.2",
    "vitest": "^1.0.0"
  }
}
